{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "moduleNameMapper": {
    "@entities/(.*)": "<rootDir>/../src/entities/$1",
    "@environment": ["<rootDir>/../src/environment"],
    "@utilities/(.*)": ["<rootDir>/../src/utilities/$1"]
  },
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
